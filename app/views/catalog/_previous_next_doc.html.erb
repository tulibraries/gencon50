<% if @search_context[:prev] || @search_context[:next] %>
  <div class='pagination-search-widgets' style="margin-top: 14px">

    <div class="page-links">
      <%= link_to_previous_document @search_context[:prev] %> |

      <%= item_page_entry_info %> |

      <%= link_to_next_document @search_context[:next] %>
    </div>
    <% if current_search_session %>
    <div class="pull-right search-widgets" style="float: right!important;">
      <%= link_back_to_catalog class: 'btn' %>

      <%=link_to t('blacklight.search.start_over'), start_over_path(current_search_session.try(:query_params) || {}), id: 'startOverLink', class: 'btn' %>
    </div>
  <% end %>
  </div>
<% end %>
